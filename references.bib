@article{castro-ginard_hunting_2019,
  title = {Hunting for Open Clusters in {{Gaia DR2}}: The {{Galactic}} Anticentre},
  shorttitle = {Hunting for Open Clusters in {{Gaia DR2}}},
  author = {{Castro-Ginard}, A. and Jordi, C. and Luri, X. and {Cantat-Gaudin}, T. and {Balaguer-N{\'u}{\~n}ez}, L.},
  year = {2019},
  month = jul,
  journal = {Astronomy \& Astrophysics},
  volume = {627},
  number = {A35},
  issn = {0004-6361, 1432-0746},
  doi = {10.1051/0004-6361/201935531},
  urldate = {2019-09-03},
  abstract = {\emph{Context{$<$}i/{$>$}. The \emph{Gaia{$<$}i/{$>$} Data Release 2 (DR2) provided an unprecedented volume of precise astrometric and excellent photometric data. In terms of data mining the \emph{Gaia{$<$}i/{$>$} catalogue, machine learning methods have shown to be a powerful tool, for instance in the search for unknown stellar structures. Particularly, supervised and unsupervised learning methods combined together significantly improves the detection rate of open clusters.\emph{Aims{$<$}i/{$>$}. We systematically scan \emph{Gaia{$<$}i/{$>$} DR2 in a region covering the Galactic anticentre and the Perseus arm (120\textdegree{} {$\leq$} \emph{l{$<$}i/{$>$} {$\leq$} 205\textdegree{} and -10\textdegree{} {$\leq$} \emph{b{$<$}i/{$>$} {$\leq$} 10\textdegree ), with the goal of finding any open clusters that may exist in this region, and fine tuning a previously proposed methodology and successfully applied to TGAS data, adapting it to different density regions.\emph{Methods{$<$}i/{$>$}. Our methodology uses an unsupervised, density-based, clustering algorithm, DBSCAN, that identifies overdensities in the five-dimensional astrometric parameter space (\emph{l{$<$}i/{$>$}, \emph{b{$<$}i/{$>$}, \emph{{$\varpi<$}i/{$>$}, \emph{{$\mu<$}i/{$>$}\textsubscript{\emph{{$\alpha<$}i/{$>$}\textsuperscript{*{$<$}sup/{$><$}sub/{$>$}, \emph{{$\mu<$}i/{$>$}\textsubscript{\emph{{$\delta<$}i/{$><$}sub/{$>$}) that may correspond to physical clusters. The overdensities are separated into physical clusters (open clusters) or random statistical clusters using an artificial neural network to recognise the isochrone pattern that open clusters show in a colour magnitude diagram.\emph{Results{$<$}i/{$>$}. The method is able to recover more than 75\% of the open clusters confirmed in the search area. Moreover, we detected 53 open clusters unknown previous to \emph{Gaia{$<$}i/{$>$} DR2, which represents an increase of more than 22\% with respect to the already catalogued clusters in this region.\emph{Conclusions{$<$}i/{$>$}. We find that the census of nearby open clusters is not complete. Different machine learning methodologies for a blind search of open clusters are complementary to each other; no single method is able to detect 100\% of the existing groups. Our methodology has shown to be a reliable tool for the automatic detection of open clusters, designed to be applied to the full \emph{Gaia{$<$}i/{$>$} DR2 catalogue.}}}}}}}}}}}}}}}}}}}}}}},
  copyright = {\textcopyright{} ESO 2019},
  langid = {english},
  keywords = {OC Finder,Read},
  file = {C\:\\Users\\Emily\\Zotero\\storage\\4DXGWEUR\\Castro-Ginard et al. - 2019 - Hunting for open clusters in Gaia DR2 the Galacti.pdf;C\:\\Users\\Emily\\Zotero\\storage\\7YCWBE58\\Castro-Ginard et al. - 2019 - Hunting for open clusters in Gaia DR2 the Galacti_annotated.pdf;C\:\\Users\\Emily\\Zotero\\storage\\IYRV576R\\aa35531-19.html}
}

@article{castro-ginard_hunting_2020,
  title = {Hunting for Open Clusters in \textbackslash textit\{\vphantom\}{{Gaia}}\vphantom\{\} {{DR2}}: \$582\$ New {{OCs}} in the {{Galactic}} Disc},
  shorttitle = {Hunting for Open Clusters in \textbackslash textit\{\vphantom\}{{Gaia}}\vphantom\{\} {{DR2}}},
  author = {{Castro-Ginard}, A. and Jordi, C. and Luri, X. and {Cid-Fuentes}, J. {\'A}lvarez and Casamiquela, L. and Anders, F. and {Cantat-Gaudin}, T. and Mongui{\'o}, M. and {Balaguer-N{\'u}{\~n}ez}, L. and Sol{\`a}, S. and Badia, R. M.},
  year = {2020},
  month = jan,
  journal = {Astronomy \& Astrophysics},
  volume = {635},
  number = {A45},
  urldate = {2020-01-22},
  abstract = {Open clusters are key targets for both Galaxy structure and evolution and stellar physics studies. Since \textbackslash textit\{Gaia\} DR2 publication, the discovery of undetected clusters has proven that our samples were not complete. Our aim is to exploit the Big Data capabilities of machine learning to detect new open clusters in \textbackslash textit\{Gaia\} DR2, and to complete the open cluster sample to enable further studies on the Galactic disc. We use a machine learning based methodology to systematically search in the Galactic disc, looking for overdensities in the astrometric space and identifying them as open clusters using photometric information. First, we use an unsupervised clustering algorithm, DBSCAN, to blindly search for these overdensities in \textbackslash textit\{Gaia\} DR2 \$(l,b,\textbackslash varpi,\textbackslash mu\_\{\textbackslash alpha\^*\},\textbackslash mu\_\textbackslash delta)\$. After that, we use a deep learning artificial neural network trained on colour-magnitude diagrams to identify isochrone patterns in these overdensities, and to confirm them as open clusters. We find \$582\$ new open clusters distributed along the Galactic disc, in the region \$|b| {$<$} 20\$. We can detect substructure in complex regions, and identify the tidal tails of a disrupting cluster UBC\textasciitilde\$274\$ of \$\textbackslash sim 3\$ Gyr located at \$\textbackslash sim 2\$ kpc. Adapting the methodology into a Big Data environment allows us to target the search driven by physical properties of the open clusters, instead of being driven by its computational requirements. This blind search for open clusters in the Galactic disc increases in a \$45\textbackslash\%\$ the number of known open clusters.},
  keywords = {ACG,Astrophysics - Astrophysics of Galaxies,Astrophysics - Solar and Stellar Astrophysics,new open cluster,open cluster discoveries,read},
  file = {C\:\\Users\\Emily\\Zotero\\storage\\YGC6V4HU\\2001.html}
}

@article{castro-ginard_hunting_2022,
  title = {Hunting for Open Clusters in {{Gaia EDR3}}: \$664\$ New Open Clusters Found with {{OCfinder}}},
  shorttitle = {Hunting for Open Clusters in {{Gaia EDR3}}},
  author = {{Castro-Ginard}, A. and Jordi, C. and Luri, X. and {Cantat-Gaudin}, T. and Carrasco, J. M. and Casamiquela, L. and Anders, F. and {Balaguer-N{\'u}{\~n}ez}, L. and Badia, R. M.},
  year = {2022},
  journal = {Astronomy \& Astrophysics},
  volume = {661},
  eprint = {2111.01819},
  pages = {A118},
  urldate = {2021-11-04},
  abstract = {The improvements in the precision of the published data in \textbackslash textit\{Gaia\} EDR3 with respect to \textbackslash textit\{Gaia\} DR2, particularly for parallaxes and proper motions, offer the opportunity to increase the number of known open clusters in the Milky Way by detecting farther and fainter objects that have so far go unnoticed. Our aim is to keep completing the open cluster census in the Milky Way with the detection of new stellar groups in the Galactic disc. We use \textbackslash textit\{Gaia\} EDR3 up to magnitude \$G = 18\$ mag, increasing in one unit the magnitude limit and therefore the search volume explored in our previous studies. We use the \textbackslash texttt\{OCfinder\} method to search for new open clusters in \textbackslash textit\{Gaia\} EDR3 using a Big Data environment. As a first step, \textbackslash texttt\{OCfinder\} identifies stellar statistical overdensities in the five dimensional astrometric space (position, parallax and proper motions) using the \textbackslash texttt\{DBSCAN\} clustering algorithm. Then, these overdensities are classified into random statistical overdensities or real physical open clusters using a deep artificial neural network trained on well-characterised \$G\$, \$G\_\{\textbackslash rm BP\} - G\_\{\textbackslash rm RP\}\$ colour-magnitude diagrams. We report the discovery of \$664\$ new open clusters within the Galactic disc, most of them located beyond \$1\$ kpc from the Sun. From the estimation of ages, distances and line-of-sight extinctions of these open clusters, we see that young clusters align following the Galactic spiral arms while older ones are dispersed in the Galactic disc. Furthermore, we find that most open clusters are located at low Galactic altitudes with the exception of a few groups older than \$1\$ Gyr. We show the success of the \textbackslash texttt\{OCfinder\} method leading to the discovery of a total of \$1\textbackslash,310\$ open clusters (joining the discoveries here with the previous ones based on \textbackslash textit\{Gaia\} DR2)[abridged]},
  archiveprefix = {arxiv},
  keywords = {Astrophysics - Astrophysics of Galaxies},
  file = {C\:\\Users\\Emily\\Zotero\\storage\\VI2WX55X\\2111.html}
}

@article{castro-ginard_milky_2021,
  title = {On the {{Milky Way}} Spiral Arms from Open Clusters in {{Gaia EDR3}}},
  author = {{Castro-Ginard}, A. and McMillan, P. J. and Luri, X. and Jordi, C. and {Romero-G{\'o}mez}, M. and {Cantat-Gaudin}, T. and Casamiquela, L. and Tarricq, Y. and Soubiran, C. and Anders, F.},
  year = {2021},
  month = may,
  journal = {Astronomy \& Astrophysics},
  volume = {652},
  eprint = {2105.04590},
  pages = {A162},
  urldate = {2021-05-14},
  abstract = {Context. The physical processes driving the formation of Galactic spiral arms are still under debate. Studies using open clusters favour the description of the Milky Way spiral arms as long-lived structures following the classical density wave theory. Current studies comparing the Gaia DR2 field stars kinematic information of the Solar neighbourhood to simulations, find a better agreement with short-lived arms with a transient behaviour. Aims. Our aim is to provide an observational, data-driven view of the Milky Way spiral structure and its dynamics using open clusters as the main tracers, and to contrast it with simulation-based approaches. We use the most complete catalogue of Milky Way open clusters, with astrometric Gaia EDR3 updated parameters, estimated astrophysical information and radial velocities, to re-visit the nature of the spiral pattern of the Galaxy. Methods. We use a Gaussian mixture model to detect overdensities of open clusters younger than 30 Myr that correspond to the Perseus, Local, Sagittarius and Scutum spiral arms, respectively. We use the birthplaces of the open cluster population younger than 80 Myr to trace the evolution of the different spiral arms and compute their pattern speed. We analyse the age distribution of the open clusters across the spiral arms to explore the differences in the rotational velocity of stars and spiral arms. Results. We are able to increase the range in Galactic azimuth where present-day spiral arms are described, better estimating its parameters by adding 264 young open clusters to the 84 high-mass star-forming regions used so far, thus increasing by a 314\% the number of tracers. We use the evolution of the open clusters from their birth positions to find that spiral arms nearly co-rotate with field stars at any given radius, discarding a common spiral pattern speed for the spiral arms explored. [abridged]},
  archiveprefix = {arxiv},
  keywords = {Astrophysics - Astrophysics of Galaxies},
  file = {C\:\\Users\\Emily\\Zotero\\storage\\R2HJREKU\\2105.html}
}

@article{castro-ginard_new_2018,
  title = {A New Method for Unveiling Open Clusters in {{Gaia}} - {{New}} Nearby Open Clusters Confirmed by {{DR2}}},
  author = {{Castro-Ginard}, A. and Jordi, C. and Luri, X. and Julbe, F. and Morvan, M. and {Balaguer-N{\'u}{\~n}ez}, L. and {Cantat-Gaudin}, T.},
  year = {2018},
  month = oct,
  journal = {Astronomy \& Astrophysics},
  volume = {618},
  pages = {A59},
  issn = {0004-6361, 1432-0746},
  doi = {10.1051/0004-6361/201833390},
  urldate = {2019-09-03},
  abstract = {\emph{Context{$<$}i/{$>$}. The publication of the \emph{Gaia{$<$}i/{$>$} Data Release 2 (\emph{Gaia{$<$}i/{$>$} DR2) opens a new era in astronomy. It includes precise astrometric data (positions, proper motions, and parallaxes) for more than 1.3 billion sources, mostly stars. To analyse such a vast amount of new data, the use of data-mining techniques and machine-learning algorithms is mandatory.\emph{Aims{$<$}i/{$>$}. A great example of the application of such techniques and algorithms is the search for open clusters (OCs), groups of stars that were born and move together, located in the disc. Our aim is to develop a method to automatically explore the data space, requiring minimal manual intervention.\emph{Methods{$<$}i/{$>$}. We explore the performance of a density-based clustering algorithm, DBSCAN, to find clusters in the data together with a supervised learning method such as an artificial neural network (ANN) to automatically distinguish between real OCs and statistical clusters.\emph{Results{$<$}i/{$>$}. The development and implementation of this method in a five-dimensional space (\emph{l{$<$}i/{$>$}, \emph{b{$<$}i/{$>$}, \emph{{$\varpi<$}i/{$>$}, \emph{{$\mu<$}i/{$>$}\textsubscript{\emph{{$\alpha<$}i/{$>$}\textsuperscript{*{$<$}sup/{$><$}sub/{$>$}, \emph{{$\mu<$}i/{$>$}\textsubscript{\emph{{$\delta<$}i/{$><$}sub/{$>$}) with the Tycho-Gaia Astrometric Solution (TGAS) data, and a posterior validation using \emph{Gaia{$<$}i/{$>$} DR2 data, lead to the proposal of a set of new nearby OCs. \emph{Conclusions{$<$}i/{$>$}. We have developed a method to find OCs in astrometric data, designed to be applied to the full \emph{Gaia{$<$}i/{$>$} DR2 archive.}}}}}}}}}}}}}}}}}}}},
  copyright = {\textcopyright{} ESO 2018},
  langid = {english},
  keywords = {OC Finder,Read}
}

@article{chan_deep-cee_2019,
