  journal = {arXiv:1911.05093 [astro-ph]},
  eprint = {1911.05093},
  primaryclass = {astro-ph},
  urldate = {2020-02-19},
  abstract = {We describe a population of young star clusters (SCs) formed in a hydrodynamical simulation of a gas-rich dwarf galaxy merger resolved with individual massive stars at sub-parsec spatial resolution. The simulation is part of the GRIFFIN (Galaxy Realizations Including Feedback From INdividual massive stars) project. The star formation environment during the simulation spans seven orders of magnitude in gas surface density and thermal pressure, and the global star formation rate surface density (\$\textbackslash Sigma\_\textbackslash mathrm\{SFR\}\$) varies by more than three orders of magnitude during the simulation. Young SCs more massive than \$M\_\{\textbackslash mathrm\{*,cl\}\}\textbackslash sim 10\^\{2.5\}\textbackslash,M\_\{\textbackslash odot\}\$ form along a mass function with a power-law index \$\textbackslash alpha\textbackslash sim-1.7\$ (\$\textbackslash alpha\textbackslash sim-2\$ for \$M\_\{\textbackslash mathrm\{*,cl\}\}\textbackslash gtrsim10\^\{3\}\textbackslash,M\_\{\textbackslash odot\}\$) at all merger phases, while the normalization and the highest SC masses (up to \$\textbackslash sim 10\^6 M\_\{\textbackslash odot\}\$) correlate with \$\textbackslash Sigma\_\textbackslash mathrm\{SFR\}\$. The cluster formation efficiency varies from \$\textbackslash Gamma\textbackslash sim20\textbackslash\%\$ in early merger phases to \$\textbackslash Gamma\textbackslash sim80\textbackslash\%\$ at the peak of the starburst and is compared to observations and model predictions. The massive SCs (\$\textbackslash gtrsim10\^4\textbackslash,M\_\{\textbackslash odot\}\$) have sizes and mean surface densities similar to observed young massive SCs. Simulated lower mass clusters appear slightly more concentrated than observed. All SCs form on timescales of a few Myr and lose their gas rapidly resulting in typical stellar age spreads between \$\textbackslash sigma\textbackslash sim0.1-2\$ Myr (\$1\textbackslash sigma\$), consistent with observations. The age spreads increase with cluster mass, with the most massive cluster (\$\textbackslash sim10\^6\textbackslash, M\_\{\textbackslash odot\}\$) reaching a spread of \$5\textbackslash, \textbackslash mathrm\{Myr\}\$ once its hierarchical formation finishes. Our study shows that it is now feasible to investigate the SC population of entire galaxies with novel high-resolution numerical simulations.},
  archiveprefix = {arxiv},
  keywords = {Astrophysics - Astrophysics of Galaxies},
  file = {C\:\\Users\\Emily\\Zotero\\storage\\W3T98FN5\\1911.html}
}

@article{latif_formation_2020,
  title = {Formation of {{Pop II}} Star Clusters in the Aftermath of a Pair Instability Supernova},
  author = {Latif, Muhammad A. and Schleicher, Dominik},
  year = {2020},
  month = oct,
  journal = {arXiv:2007.13213 [astro-ph]},
  eprint = {2007.13213},
  primaryclass = {astro-ph},
  urldate = {2020-10-07},
  abstract = {Pop II stars formed a few hundred million years after the Big Bang were key drivers of cosmic reionization and building blocks of high redshift galaxies. How and when these stars formed is a subject of ongoing research. We conduct cosmological radiation hydrodynamical simulations to investigate the formation of Pop II star clusters in dark matter halos forming at \$z=10-25\$ in the aftermath of a pair instability supernova (PISN). Our simulations model the formation of Pop III and Pop II stars in a self-consistent manner along with their radiative, chemical and SN feedback in halos of \$\textbackslash rm 5 \textbackslash times 10\^5- 7 \textbackslash times 10\^7\textasciitilde M\_\{\textbackslash odot\}\$. We find that a PISN evacuates the gas from halos \$\textbackslash rm \textbackslash leq 3 \textbackslash times 10\^\{6\}\textasciitilde M\_\{\textbackslash odot\}\$ and thereafter shuts off in situ star formation for at-least 30 Myr. Pop II stellar clusters of \$\textbackslash rm 923\textasciitilde M\_\{\textbackslash odot\}\$ and \$\textbackslash rm 6800\textasciitilde M\_\{\textbackslash odot\}\$ form in halos of \$\textbackslash rm 3.5 \textbackslash times 10\^7\textasciitilde M\_\{\textbackslash odot\}\$ and \$\textbackslash rm 7.2 \textbackslash times 10\^7\textasciitilde M\_\{\textbackslash odot\}\$, respectively. The mode of star formation is highly episodic and mainly regulated by Pop II SN feedback. The average star formation rates are \$\textbackslash rm 10\^\{-5\}-10\^\{-4\}\textasciitilde M\_\{\textbackslash odot\}/yr\$ and the star formation efficiency is less than 1\textbackslash\%.},
  archiveprefix = {arxiv},
