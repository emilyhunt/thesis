  title = {Reanalysis of Nearby Open Clusters Using {{Gaia DR1}}/{{TGAS}} and {{HSOY}}},
  author = {Yen, Steffi X. and Reffert, Sabine and Schilbach, Elena and R{\"o}ser, Siegfried and Kharchenko, Nina V. and Piskunov, Anatoly E.},
  year = {2018},
