@article{negueruela_massive_2021,
  title = {A Massive Open Cluster Hiding in Full Sight},
  author = {Negueruela, Ignacio and Chen{\'e}, Andr{\'e}-Nicolas and Tabernero, Hugo M. and Dorda, Ricardo and Borissova, Jura and Marco, Amparo and Kurtev, Radostin},
  year = {2021},
