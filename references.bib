@article{ramirez_vvv_2021,
  title = {The {{VVV Open Cluster Project}}. {{Near-infrared}} Sequences of {{NGC6067}}, {{NGC6259}}, {{NGC4815}}, {{Pismis18}}, {{Trumpler23}}, and {{Trumpler20}}},
  author = {Ram{\'i}rez, K. Pe{\~n}a and {Gonz{\'a}lez-Fern{\'a}ndez}, C. and Chen{\'e}, A.-N. and Alegr{\'i}a, S. Ram{\'i}rez},
