@article{monteiro_distribution_2021,
  title = {The Distribution of Open Clusters in the {{Galaxy}}},
  author = {Monteiro, H. and Barros, D. A. and Dias, W. S. and L{\'e}pine, J. R. D.},
  year = {2021},
