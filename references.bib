  author = {Kovaleva, Dana and Ishchenko, Marina and Postnikova, Ekaterina and Berczik, Peter and Piskunov, Anatoly E. and Kharchenko, Nina V. and Polyachenko, Evgeny and Reffert, Sabine and Sysoliatina, Kseniia and Just, Andreas},
  year = {2020},
