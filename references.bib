@article{kumaran_active_2018,
  title = {Active {{Metric Learning}} for {{Supervised Classification}}},
  author = {Kumaran, Krishnan and Papageorgiou, Dimitri and Chang, Yutong and Li, Minhan and Tak{\'a}{\v c}, Martin},
