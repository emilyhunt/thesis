  file = {C\:\\Users\\Emily\\Zotero\\storage\\TD72K56J\\Randich et al. - 2018 - The Gaia-ESO Survey open clusters in Gaia-DR1 - A.pdf;C\:\\Users\\Emily\\Zotero\\storage\\D3VRB2RK\\aa31738-17.html}
}

