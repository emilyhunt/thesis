@article{perren_analysis_2022,
  title = {An Analysis of the Most Distant Catalogued Open Clusters -- {{Re-assessing}} Fundamental Parameters with {{Gaia EDR3}} and \$\textbackslash texttt\{\vphantom\}{{ASteCA}}\vphantom\{\}\$},
  author = {Perren, G. I. and Pera, M. S. and Navone, H. D. and V{\'a}zquez, R. A.},
  year = {2022},
