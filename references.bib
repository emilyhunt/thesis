  author = {Anders, F. and Khalatyan, A. and Queiroz, A. B. A. and Chiappini, C. and Ard{\`e}vol, J. and Casamiquela, L. and Figueras, F. and {Jim{\'e}nez-Arranz}, {\'O} and Jordi, C. and Mongui{\'o}, M. and {Romero-G{\'o}mez}, M. and Altamirano, D. and Antoja, T. and Assaad, R. and {Cantat-Gaudin}, T. and {Castro-Ginard}, A. and Enke, H. and Girardi, L. and Guiglion, G. and Khan, S. and Luri, X. and Miglio, A. and Minchev, I. and Ramos, P. and Santiago, B. X. and Steinmetz, M.},
  year = {2021},
