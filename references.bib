  year = {2019},
  month = dec,
  journal = {arXiv:1912.06657 [astro-ph]},
  eprint = {1912.06657},
  primaryclass = {astro-ph},
  urldate = {2020-01-02},
  abstract = {We present the stellar population, using \{\textbackslash it Gaia\}\textbackslash,DR2 parallax, kinematics, and photometry, of the young (\$\textbackslash sim 100\$\textasciitilde Myr), nearby (\$\textbackslash sim 230\$\textasciitilde pc) open cluster, Blanco1. A total of 644 member candidates are identified via the unsupervised machine learning method \textbackslash textsc\{StarGO\} to find the clustering in the 5-dimensional position and proper motion parameter (\$X\$, \$Y\$, \$Z\$, \$\textbackslash mu\_\textbackslash alpha \textbackslash cos\textbackslash delta\$, \$\textbackslash mu\_\textbackslash delta\$) space. Within the tidal radius of \$10.0 \textbackslash pm 0.3\$\textasciitilde pc, there are 488 member candidates, 3 times more than those outside. A leading tail and a trailing tail, each of 50--60\textasciitilde pc in the Galactic plane, are found for the first time for this cluster, with stars further from the cluster center streaming away faster, manifest stellar stripping. Blanco1 has a total detected mass of \$285\textbackslash pm32\$\textasciitilde M\$\_\textbackslash odot \$ with a mass function consistent with a slope of \$\textbackslash alpha=1.35\textbackslash pm0.2\$ in the sense of \$dN/dm \textbackslash propto m\^\{-\textbackslash alpha\}\$, in the mass range of 0.25--2.51\textasciitilde M\$\_\textbackslash odot \$, where \$N\$ is the number of members and \$m\$ is stellar mass. A Minimum Spanning Tree (\$\textbackslash Lambda\_\{\textbackslash rm MSR\}\$) analysis shows the cluster to be moderately mass segregated among the most massive members (\$\textbackslash gtrsim 1.4\$\textasciitilde M\$\_\textbackslash odot\$), suggesting an early stage of dynamical disintegration.},
  archiveprefix = {arxiv},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Solar and Stellar Astrophysics,blanco 1,mass segregation,Read,sc structure,self-organising map,stargo,tidal tails},
  file = {C\:\\Users\\Emily\\Zotero\\storage\\WS7AA65L\\1912.html}
}

@article{zhang_radial_2021,
  title = {Radial {{Migration}} from {{Metallicity Gradient}} of {{Open Clusters}} and {{Outliers}}},
  author = {Zhang, Haopeng and Chen, Yuqin and Zhao, Gang},
  year = {2021},
  month = jun,
  journal = {arXiv:2106.12841 [astro-ph]},
  eprint = {2106.12841},
  primaryclass = {astro-ph},
  urldate = {2021-07-21},
  abstract = {Radial migration is an important process in the evolution of the Galactic disk. The metallicity gradient of open clusters and its outliers provide an effective way to probe for this process. In this work, we compile metallicity, age, and kinematic parameters for 225 open clusters and carry out a quantitative analysis of radial migration via the calculated migration distances. Based on clusters with age \${$<$} 0.5\$ Gyr, we obtain the present-day metallicity gradient of \$-0.074 \textbackslash pm 0.007\$ dex/kpc. Along this gradient distributes three sequences, and clusters in the upper, the middle, and the lower groups are found to be old outward-migrators, in-situ clusters, and inward-migrators, respectively. The migration distance increases with age, but its most effective time is probably less than 3 Gyr. The metallicity gradient breaks out at \$R\_g\$ (guiding center radius) \$\textbackslash sim11.5\$ kpc, which is caused by the lack of young open clusters in the outer disk and the presence of old outward-migrators in the upper sequence. It shows that this boundary is related to the different effects of radial migration between the inner and outer disks. We also found many special open clusters in and near the outer disk of \$R {$>$} 11\$ kpc and a steeper metallicity gradient from the inner disk of \$R\_g {$<$} 7\$ kpc, which tells a complicated evolution history of the Galactic disk by different effects of stellar radial migration.},
  archiveprefix = {arxiv},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Solar and Stellar Astrophysics},
  file = {C\:\\Users\\Emily\\Zotero\\storage\\TD3XLHVZ\\2106.html}
}

@article{zhong_exploring_2020,
  title = {Exploring Open Cluster Properties with {{Gaia}} and {{LAMOST}}},
  author = {Zhong, Jing and Chen, Li and Wu, Di and Li, Lu and Bai, Leya and Hou, Jinliang},
  year = {2020},
  month = jun,
  journal = {arXiv:2006.06929 [astro-ph]},
  eprint = {2006.06929},
  primaryclass = {astro-ph},
  urldate = {2020-06-15},
  abstract = {In Gaia DR2, the unprecedented high-precision level reached in sub-mas for astrometry and mmag for photometry. Using cluster members identified with these astrometry and photometry in Gaia DR2, we can obtain a reliable determination of cluster properties. However, because of the shortcoming of Gaia spectroscopic observation in dealing with densely crowded cluster region, the number of radial velocity and metallicity for cluster member stars from Gaia DR2 is still lacking. In this study, we aim to improve the cluster properties by combining the LAMOST spectra. In particular, we provide the list of cluster members with spectroscopic parameters as an add-value catalog in LAMOST DR5, which can be used to perform detailed study for a better understanding on the stellar properties, by using their spectra and fundamental properties from the host cluster. We cross-matched the spectroscopic catalog in LAMOST DR5 with the identified cluster members in Cantat-Gaudin et al.2018 and then used members with spectroscopic parameters to derive statistical properties of open clusters. We obtained a list of 8811 members with spectroscopic parameters and a catalog of 295 cluster properties. In addition, we study the radial and vertical metallicity gradient and age-metallicity relation with the compiled open clusters as tracers, finding slopes of -0.053\$\textbackslash pm\$0.004 dex kpc\$\^\{-1\}\$, -0.252\$\textbackslash pm\$0.039 dex kpc\$\^\{-1\}\$ and 0.022\$\textbackslash pm\$0.008 dex Gyr\$\^\{-1\}\$, respectively. Both slopes of metallicity distribution relation for young clusters (0.1 Gyr {$<$} Age {$<$} 2 Gyr) and the age-metallicity relation for clusters within 6 Gyr are consistent with literature results. In order to fully study the chemical evolution history in the disk, more spectroscopic observations for old and distant open clusters are needed for further investigation.},
  archiveprefix = {arxiv},
  keywords = {Astrophysics - Astrophysics of Galaxies},
  file = {C\:\\Users\\Emily\\Zotero\\storage\\3RFSATGP\\2006.html}
}

@article{zhou_galactic_2021,
  title = {Galactic Open Cluster {{Cepheids}} -- a Census Based on {{Gaia EDR3}}},
  author = {Zhou, Xiaoyue and Chen, Xiaodian},
  year = {2021},
  month = apr,
  journal = {arXiv:2104.11929 [astro-ph]},
  eprint = {2104.11929},
  primaryclass = {astro-ph},
  urldate = {2021-04-28},
  abstract = {In the Gaia era, the membership analysis and parameter determination of open clusters (OCs) are more accurate. We performed a census of OC's classical Cepheids based on Gaia Early Data Release 3 (EDR3) and obtained a sample of 33 OC Cepheids fulfilling the constraints of the spatial position, proper motion, parallax and evolution state. 13 of 33 OC Cepheids are newly discovered. Among them, CM Sct is the first first-crossing Cepheids with direct evidence of evolution. DP Vel is likely a fourth- or fifth-crossing Cepheids. Based on independent distances from OCs, W\_1-band period-luminosity relation of Cepheids is determined with a 3.5\% accuracy: {$<$}MW1{$>$} = -(3.274 +- 0.090) log P - (-2.567 +- 0.080). The Gaia-band period-Wesenheit relation agrees well with Ripepi et al. (2019). A direct period-age relation for fundamental Cepheids are also determined based on OC's age, that is log t = -(0.638 +- 0.063) log P + (8.569 +- 0.057).},
  archiveprefix = {arxiv},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Solar and Stellar Astrophysics},
  file = {C\:\\Users\\Emily\\Zotero\\storage\\WFTXH8BK\\2104.html}
}

